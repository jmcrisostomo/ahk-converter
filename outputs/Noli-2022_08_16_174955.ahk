Noli[7863] := "Datapuwa't ac{U+00F3}ng nagm{U+00E1}na n~g cayamanan, m~ga pagca-al{U+00E1}ng-{U+00E1}lang n~g"
Noli[7864] := "c{U+00E1}puw{U+00E0}, ac{U+00F3}'y nag-{U+00E1}ral, n~gay{U+00F3}'y is{U+00E1}ng mah{U+00ED}rap ac{U+00F3}, at hind{U+00EE} ac{U+00F3}"
Noli[7865] := "pinagcatiwal{U+0E2A}n n~g l{U+00E1}long wal{U+00E1}ng cabuluh{U+00E1}ng tungc{U+00FA}lin, at ang sinasabi"
Noli[7866] := "n~g lah{U+00E1}t: ""{U+00A1}Iy{U+00E1}'y is{U+00E1}ng ul-{U+00F3}l, iy{U+00E1}'y hind{U+00EE} nacauunaw{U+00E0} cung an{U+00F3} ang"
Noli[7867] := "pamumuhay{!}"" Tinatawag ac{U+00F3} n~g curang ""fil{U+00F3}sofo"" n~g palib{U+0E1C}, na ang"
Noli[7868] := "ipinahihiwatig ay ac{U+00F3}'y is{U+00E1}ng madaldal na ipinagm{U+00E1}mayabang ang m~ga"
