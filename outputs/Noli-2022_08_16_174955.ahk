Noli[1371] := """Romeo at Julieta"", ""Hamlet,"" ""Otelo,"" ""Ang Mercader sa Venecia,"" ""Ang"
Noli[1372] := "panaguinip n~g is{U+00E1}ng gab{U+00ED}ng tag-araw"" at iba pa.--P.H.P."
Noli[1373] := "[97] Ang caharian n~g China."
Noli[1374] := "[98] Pan~galaw{U+00E1}ng libro n~g Pentatesco ni Mois{U+00E9}s.--Ang paglalacbay sa"
Noli[1375] := "ibang lupain n~g m~ga t{U+0FAB}{U+00F2} sa is{U+00E1}ng naci{U+00F3}n {U+00F3} b{U+00E1}yan, na siy{U+00E1}ng cahulug{U+00E1}n"
Noli[1376] := "dito."
