Noli[7908] := "--Hind{U+00EE} ac{U+00F4} lubh{U+00E1}ng mapangl{U+00E1}win sa pag-i{U+00ED}sip n~g mangyayari sa an{U+00F3} mang"
Noli[7909] := "b{U+00E1}gay, at sa g{U+00E1}nang {U+00E1}kin ay hind{U+00EE} nam{U+00E1}n napacapan~ganib ang pamumuhay sa"
Noli[7910] := "{U+00E1}king lupa{U+00ED}n,--ang is{U+00ED}nag{U+00F3}t ni Ibarrang n~gumin~git{U+00EE}.--Inaacal{U+00E0} cong"
